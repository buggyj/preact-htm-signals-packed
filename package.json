  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buggyj/preact-htm-signals-packed.git"
  },
  "homepage": "https://github.com/buggyj/preact-htm-signals-packed",
  "keywords": [
  ],
  "devDependencies": {
    "@preact/signals": "^1.3.0",
    "htm": "3.1.1",
    "microbundle": "^0.15.1",
    "preact": "^10.23.1"
  }
}
