{
  "name": "preact-htm-signals-packed ",
  "version": "0.1.0",
  "description": "Preact, HTM and Preact Signals in a single module",
  "type": "module",
  "main": "dist/standalone.umd.js",
  "module": "dist/standalone.js",
  "umd:main": "dist/standalone.umd.js",
